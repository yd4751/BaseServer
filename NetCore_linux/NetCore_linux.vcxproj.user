<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <RemoteDebuggerCommand>$(RemoteRootDir)/$(SolutionName)/netcore</RemoteDebuggerCommand>
    <RemoteDebuggerWorkingDirectory>$(RemoteRootDir)/$(SolutionName)/</RemoteDebuggerWorkingDirectory>
    <DebuggerFlavor>LinuxDebugger</DebuggerFlavor>
    <AdditionalSymbolSearchPaths>$(SolutionDir)..\code\;$(AdditionalSymbolSearchPaths)</AdditionalSymbolSearchPaths>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
    <RemoteDebuggerCommand>$(RemoteRootDir)/$(ProjectName)/$(ProjectName)/netcore</RemoteDebuggerCommand>
    <RemoteDebuggerWorkingDirectory>$(RemoteRootDir)/$(ProjectName)</RemoteDebuggerWorkingDirectory>
    <DebuggerFlavor>LinuxDebugger</DebuggerFlavor>
  </PropertyGroup>
</Project>